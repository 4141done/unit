NXT_LIB_POLARSSL_SRCS="src/nxt_polarssl.c"

NXT_LIB_EPOLL_SRCS="src/nxt_epoll.c"
NXT_LIB_KQUEUE_SRCS="src/nxt_kqueue.c"
